#include <stdio.h>
#include <stdlib.h>
#include <string.h>



int main(int argc, char *argv[])
{
    int a = rand()%10 + 4; 
    int t = rand()%10 + 5; 
    int i, j;
    int UsualMass[a][t];

    srand(5);

    for ( i = 0; i < a; i++ )
    {
	    for ( j = 0; j < t; j++ )
	    UsualMass[i][j] = rand()%100;
    }
        printf( "Item 1:\n" );
    for ( i = 0; i < a; i++ )
    {
	    for ( j = 0; j < t; j++ )
        {
    	    printf( "[%d][%d]=%d ", i, j, UsualMass[i][j] );
        }
        printf( "\n" );
    }
    printf( "\n" );

    int mazer;

    for ( j = 0; j < t; j++ )
	{
	 	mazer = UsualMass[0][j];
	 	UsualMass[0][j] = UsualMass[a-1][j];
		UsualMass[a-1][j] = mazer;
	}

    for ( j = 0; j < (t/2); j++ )
	{
		mazer = UsualMass[0][j];
		UsualMass[0][j] = UsualMass[0][t-1-j];
		UsualMass[0][t-1-j] = mazer;
	}

    for ( j = 0; j < (t/2); j++ )
	{
	    mazer = UsualMass[a-1][j];
	    UsualMass[a-1][j] = UsualMass[a-1][t-1-j];
	    UsualMass[a-1][t-1-j] = mazer;
	}

    printf( "Item 2:\n" );

  for ( i = 0; i < a; i++ )
    {
        for ( j = 0; j < t; j++ )
        {
            printf( "[%d][%d]=%d ", i, j, UsualMass[i][j] );
        }
        printf( "\n" );
    }
 
    int min = UsualMass[0][0];
    int I = 0;
    int J = 0;
    for ( i = 0; i < a; i++ )
    {
            for ( j = 0; j < t; j++ )
        {
            if (min > UsualMass[i][j])
            {
        	    min = UsualMass[i][j];
        	    I = i;
        	    J = j;
            }
        }
    }

    int M = a-1;
    int l = t-1;
    int MassOne[M][l];


    for (i = 0; i < M; i++)
    {
            for (j = 0; j < l; j++)
        {
                if (i >= I && j >= J)
            {
                    MassOne[i][j] = UsualMass[i+1][j+1];
            }
                else if (i >= I && j < J)
            {
                    MassOne[i][j] = UsualMass[i+1][j];
            }
                else if (i < I && j >= J)
            {
                    MassOne[i][j+1] = UsualMass[i][j+1];
            }
                else
            {
                    MassOne[i][j] = UsualMass[i][j];
            }
        }
    }
    printf( "Item 3:\n" );
    for ( i = 0; i < M; i++ )
    {
        for ( j = 0; j < l; j++ )
        {
            printf( "[%d][%d]=%d ", i, j, MassOne[i][j] );
        }
        printf( "\n" );
    }


    int G, S;
        printf( "Enter the number of columns in the matrix.\n" );
    G = l;

    scanf("%d", &S);

  int MassTwo[G][S];
    for ( i = 0; i < G; i++ )
    {
     for ( j = 0; j < S; j++ )
        {
    	    printf("Enter [%d][%d] elements of the array.",i ,j);
            scanf("%d", &MassTwo[i][j]);
        }
    }
        printf( "Item 4:\n" );
    for ( i = 0; i < G; i++ )
    {
            for ( j = 0; j < S; j++ )
        {
            printf( "[%d][%d]=%d ", i, j, MassTwo[i][j] );
        }
        printf( "\n" );
    }
   

 int UsualMass3[M][S];
 int q;
 int Jedi = 0;

    for ( i = 0; i < M; i++ )
    {
        for ( j = 0; j < S; j++ )
        {
    	    for ( q = 0; q < G; q++ )
    	    {
    		    Jedi = Jedi + MassOne[i][q]*MassTwo[q][j];
    		    printf( "%d = %d + %d*%d", Jedi, Jedi, MassOne[i][q], MassTwo[q][j]);
    	    }
            UsualMass3[i][j]=Jedi;
            Jedi = 0;
        }
    }
    printf( "Item 5:\n" );
    for ( i = 0; i < M; i++ )
    {
        for ( j = 0; j < S; j++ )
        {
            printf( "[%d][%d]=%d ", i, j, UsualMass3[i][j] );
        }
            printf( "\n" );
    }
    return 0;
}
